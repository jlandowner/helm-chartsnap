# Source: app1/templates/deployment.yaml
apiVersion: apps/v1
kind: Deployment
metadata:
  name: chartsnap-app1
  labels:
    helm.sh/chart: '###DYNAMIC_FIELD###'
    app.kubernetes.io/name: app1
    app.kubernetes.io/instance: chartsnap
    app.kubernetes.io/version: "1.16.0"
    app.kubernetes.io/managed-by: Helm
spec:
  selector:
    matchLabels:
      app.kubernetes.io/name: app1
      app.kubernetes.io/instance: chartsnap
  template:
    metadata:
      labels:
        helm.sh/chart: app1-0.1.0
        app.kubernetes.io/name: app1
        app.kubernetes.io/instance: chartsnap
        app.kubernetes.io/version: "1.16.0"
        app.kubernetes.io/managed-by: Helm
    spec:
      serviceAccountName: chartsnap-app1
      securityContext: {}
      containers:
      - name: app1
        securityContext: {}
        image: "nginx:1.16.0"
        imagePullPolicy: IfNotPresent
        ports:
        - name: http
          containerPort: 80
          protocol: TCP
        livenessProbe:
          httpGet:
            path: /
            port: http
        readinessProbe:
          httpGet:
            path: /
            port: http
        resources: {}
